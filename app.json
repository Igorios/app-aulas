{
  "expo": {
    "name": "Aplicativo de escola",
    "slug": "frontend",
    "scheme": "frontend",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "extra": {
      "eas": {
        "projectId": "095f7444-5db9-4a64-867b-5bbd99a5a933"
      }
    },
    "splash": {
      "image": "./assets/capa.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "icon": "./assets/capa.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/capa.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.igorios.frontend",
      "packageType": "APK"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
